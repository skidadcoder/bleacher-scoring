{
  "expo": {
    "name": "Bleacher Scoring - Volleyball Edition",
    "description": "Live user driven scoreboards for volleyball games.",
    "slug": "bleacher-scoring-volleyball",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "bleacherscoringvolleyball",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bleachertechnologies.bleacherscoringvolleyball",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Bleacher Scoring uses your location to find games nearby.",
        "NSPhotoLibraryUsageDescription": "Bleacher Scoring allows you to upload photos to the scoreboard."
      }
    },
    "android": {
      "package": "com.bleachertechnologies.bleacherscoringvolleyball"
    }
  }
}